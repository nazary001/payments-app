{
  "name": "payments-app",
  "version": "0.0.1",
  "description": "Payments Application",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --copy-files --out-dir dist",
    "postbuild:server": "if test \"$DATABASE_RESET\" = \"true\"; then npm run dbsetup; fi",
    "cleanup": "rimraf ./dist",
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "dev:client": "webpack serve --config client/webpack/dev.config.js",
    "dev:server": "cross-env LOG_LEVEL=debug PORT=4000 nodemon --inspect server/server.js",
    "dbsetup": "node ./server/dbsetup.js",
    "preserve": "npm run build",
    "serve": "npm start",
    "start": "node dist/server.js",
    "lint": "npm run lint:eslint && npm run lint:prettier -- --check",
    "lint:eslint": "eslint .",
    "lint:fix": "npm run lint:eslint -- --fix && npm run lint:prettier -- --write",
    "lint:prettier": "prettier .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ukrainian-Bug-Hunters/payments-app.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.16.7",
    "@types/react-router-dom": "^5.3.3",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.33.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "grommet": "^2.25.3",
    "grommet-icons": "^4.8.0",
    "history": "^5.3.0",
    "html-webpack-plugin": "^5.4.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest-environment-jsdom": "^29.4.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "react-scripts": "^2.1.3",
    "rimraf": "^4.1.2",
    "style-loader": "^3.3.1",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "socket.io-client": "^4.6.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "socket.io": "^4.6.0",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  }
}
